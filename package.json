    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.15",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.2",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
